{
	"appId": "com.mekadevelopments.cliente",
	"appName": "Meka",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": false
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_ionic_logo",
			"iconColor": "#4d4d4d",
			"sound": "beep.wav"
		},
		"CapacitorCookies": {
			"enabled": true
		}
	},
	"android": {
		"allowMixedContent": true
	}
}
